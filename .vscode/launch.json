    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Run Tests",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "console": "integratedTerminal",
            "args": [
                "test"
            ]
        },
        {
            "name": "Python Debugger: process_game_outcomes",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "console": "integratedTerminal",
            "args": [
                "process_game_outcomes",
                "--hours-back=720"
            ]
        },
        {
            "name": "Python Debugger: send_reminder_emails",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "console": "integratedTerminal",
            "args": [
                "send_reminder_emails",
                //"--dry-run"
                //"--force-send-user=hoop"
            ]
        },
        {
            "name": "Python Debugger: update_dbb_matches",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "console": "integratedTerminal",
            "args": [
                "update_dbb_matches"
            ]
        },
        {
            "name": "Python Debugger: Django",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "console": "integratedTerminal",
            "args": [
                "runserver",
            ],
            "django": true,
            "justMyCode": true,
            "env": {
            }
        }
    ]
}